/* pages/profile/historyOrder.wxss */

@import '../../templates/myorder-item/myorder-item.wxss';

/* 订单头部 */
.title {
    padding: 10rpx 20rpx;
    background-color: white;
    border-top: 1px #f1f1f1 solid;
    border-bottom: 1px #f1f1f1 solid;
    font-size: 30rpx;
    margin-top: 20rpx;
}

/* 订单cell */
.appointment-cell {
  background-color: white;
  width: 95%;
  padding: 2.5%;
  position:relative;
  border-bottom: 1px #f5f5f5 solid;
  font-size: 30rpx;
}
.cellImg {
  width: 125rpx;
  height: 125rpx;
  margin-right: 10rpx;
  position: absolute;
}
.appointment-cell .text {
  display: inline-block;
  width: 75%;  
  padding-left: 150rpx;
}

.price-view {
  font-size: 26rpx;
  text-align: right;
  background-color: white;
  padding: 20rpx;
}
.price-text {
  color: #333;
}
.preprice-text {
  margin: 0 20rpx;
}