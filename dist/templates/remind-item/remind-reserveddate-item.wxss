

/* 弹窗 */
.selectDate-bgview {
    width: 100%;
    height: 100%;
    background-color: rgba(1,1,1,0.6);
    position: fixed;
    bottom: 0;
    z-index: 10;
}
.datepicker-view {
    width: 90%;
    background-color: white;
    border-radius: 10rpx;
    margin: 30% auto;
}

.datepicker-view .title {
    padding: 20rpx 30rpx;
    font-size:38rpx;
}
.datepicker-view .title input{
    border-bottom: 1px #E5E5E5 solid;
    display:inline-block;
    font-size:36rpx;
}
.picker {
  margin-bottom: 30rpx;
}
.picker picker-view {
  width: 100%; 
  height: 150px;
}
.picker-btns {
  bottom: 0;
  margin-top: 20rpx;
  padding: 20rpx 0;
  border-top: 1px #f1f1f1 solid;
  text-align: center;
}
.picker-view {
	color:#FF7CA0;
	text-align: center;
}